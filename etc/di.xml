<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Yu\NovaPoshta\Api\Data\CityInterface" type="Yu\NovaPoshta\Model\City" />
    <preference for="Yu\NovaPoshta\Api\Data\WarehouseInterface" type="Yu\NovaPoshta\Model\Warehouse" />
    <preference for="Yu\NovaPoshta\Api\Data\StreetInterface" type="Yu\NovaPoshta\Model\Street"/>

    <preference for="Yu\NovaPoshta\Api\CityRepositoryInterface" type="Yu\NovaPoshta\Model\CityRepository" />
    <preference for="Yu\NovaPoshta\Api\WarehouseRepositoryInterface" type="Yu\NovaPoshta\Model\WarehouseRepository" />
    <preference for="Yu\NovaPoshta\Api\StreetRepositoryInterface" type="Yu\NovaPoshta\Model\StreetRepository"/>

    <preference for="Yu\NovaPoshta\Api\Data\CitySearchResultsInterface" type="Magento\Framework\Api\SearchResults" />
    <preference for="Yu\NovaPoshta\Api\Data\WarehouseSearchResultsInterface" type="Magento\Framework\Api\SearchResults" />
    <preference for="Yu\NovaPoshta\Api\Data\StreetSearchResultsInterface" type="Magento\Framework\Api\SearchResults"/>

    <type name="Magento\Quote\Api\ShipmentEstimationInterface">
        <plugin name="novaposhta_check_plugin" type="Yu\NovaPoshta\Plugin\NovaPoshtaCheckPlugin" sortOrder="100" disabled="false"/>
    </type>
    <type name="Magento\QuoteGraphQl\Model\Cart\GetShippingAddress">
        <plugin name="novaposhta_address_plugin" type="Yu\NovaPoshta\Plugin\GetShippingAddressGraphQl" sortOrder="100" disabled="false"/>
    </type>
    <type name="Magento\Quote\Model\ShippingAddressManagement">
        <plugin disabled="false"
                name="extfields"
                sortOrder="10"
                type="Yu\NovaPoshta\Plugin\ShippingAddressManagementPlugin"/>
    </type>
    <type name="Magento\Sales\Api\OrderAddressRepositoryInterface">
        <plugin disabled="false"
                name="extfields"
                sortOrder="10"
                type="Yu\NovaPoshta\Plugin\OrderAddressRepository"/>
    </type>

    <type name="Magento\Framework\Console\CommandList">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="commandexample_somecommand" xsi:type="object">Yu\NovaPoshta\Console\Command\ImportCommand</item>
            </argument>
        </arguments>
    </type>
</config>
